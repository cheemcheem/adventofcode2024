    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "typescript-eslint": "^8.17.0"
  },
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx,mts}": [
      "eslint --fix"
